.has-tooltip{ class: "limit-box limit-box-#{objects.to_s.singularize} prepend-left-5", data: { title: "Project has too many #{label_for_message} to search"} }
  .limit-icon
    - if objects == :branches
      = icon('code-fork')
    - else
      = icon('tag')
  .limit-message
    %span #{objects.capitalize} unavailable
