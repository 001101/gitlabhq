%article.readme-holder#README
  %h4.readme-file-title
    %i.icon-file
    = readme.name
  .wiki
    - if gitlab_markdown?(readme.name)
      = preserve do
        = markdown(readme.data)
    - elsif markup?(readme.name)
      = render_markup(readme.name, readme.data)
    - else
      = simple_format(readme.data)
